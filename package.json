{
    "name": "riva-launcher",
    "productName": "Riva Launcher",
    "icon": "public/riva.ico",
    "exports": {
        "static/": "./public/",
        "css/": "./src/renderer/styles/css/",
        "scss/": "./src/renderer/styles/scss/",
        "sass/": "./src/renderer/styles/sass/",
        "env/": "./env/"
    },
    "version": "1.0.0",
    "description": "Riva Launcher",
    "main": "./dist/index.cjs",
    "scripts": {
        "clean-install": "rm -r node_modules && rm package-lock.json && npm i --legacy-peer-deps",
        "mockdb": "json-server --watch ./data/db.json --port 3009 --host 0.0.0.0",
        "run-electron": "export VITE_APP_ENV=dev || SET \"VITE_APP_ENV=dev\" && node ./tools/vite/vite.js",
        "run-electron-dev": "export NODE_ENV=dev || SET \"NODE_ENV=dev\" && export MODE=dev || SET \"MODE=dev\" && npm run run-electron",
        "run-electron-staging": "export NODE_ENV=staging || SET \"NODE_ENV=staging\" && export MODE=staging || SET \"MODE=staging\" && npm run run-electron",
        "run-electron-prod": "export NODE_ENV=prod || SET \"NODE_ENV=prod\" && export MODE=prod || SET \"MODE=prod\" && npm run run-electron",
        "run-vite": "cd src/renderer && vite",
        "dev": "run-p run-vite run-electron-dev mockdb",
        "staging": "run-p run-vite run-electron-staging mockdb",
        "prod": "run-p run-vite run-electron-prod mockdb",
        "build-preload": "vite build --config src/preload/vite.config.js",
        "build-renderer": "vite build --config src/renderer/vite.config.js",
        "build-main": "vite build --config src/main/vite.config.js",
        "build-splash": "vite build --config src/splash/vite.config.js",
        "build-dev": "export NODE_ENV=dev || SET \"NODE_ENV=dev\" && export MODE=dev || SET \"MODE=dev\" && npm run build-renderer && npm run build-preload && npm run build-splash",
        "build-staging": "export NODE_ENV=staging || SET \"NODE_ENV=staging\" && export MODE=staging || SET \"MODE=staging\" && npm run build-renderer && npm run build-preload && npm run build-splash",
        "build-prod": "export NODE_ENV=prod || SET \"NODE_ENV=prod\" && export MODE=prod || SET \"MODE=prod\" && npm run build-renderer && npm run build-preload && npm run build-splash",
        "pack": "electron-builder --dir",
        "electron-build": "electron-builder --config ./tools/electron-builder/electron.builder.config.js",
        "publish-dev": "export VITE_APP_ENV=publish || SET \"VITE_APP_ENV=publish\" && run-s build-dev build-main electron-build",
        "publish-staging": "export VITE_APP_ENV=publish || SET \"VITE_APP_ENV=publish\" && run-s build-staging build-main electron-build",
        "publish-prod": "export VITE_APP_ENV=publish || SET \"VITE_APP_ENV=publish\" && run-s build-prods build-main electron-build"
    },
    "keywords": [
        "riva",
        "launcher"
    ],
    "contributors": [
        {
            "name": "Necati Mert",
            "email": "necatiu56@gmail.com"
        }
    ],
    "author": {
        "name": "Necati Mert",
        "email": "necatiu56@gmail.com"
    },
    "license": "MIT",
    "dependencies": {
        "@coreui/icons": "^3.0.0-alpha.0",
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@material-ui/core": "^4.12.4",
        "@mui/icons-material": "^5.6.0",
        "@mui/material": "^5.6.0",
        "@reduxjs/toolkit": "^1.8.1",
        "@vitejs/plugin-react": "^1.3.0",
        "axios": "^0.26.1",
        "dotenv": "^16.0.0",
        "events": "^3.3.0",
        "jquery": "^3.6.0",
        "path": "^0.12.7",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-html-parser": "^2.0.2",
        "react-minimize": "^0.16.3",
        "react-redux": "^7.2.8",
        "react-router-dom": "^6.3.0",
        "react-router-transition": "^2.1.0",
        "react-social-icons": "^5.13.0",
        "react-testing-library": "^8.0.1",
        "react-verification-code-input": "^1.2.9",
        "rollup-plugin-node-builtins": "^2.1.2",
        "sass": "^1.49.11",
        "styled-components": "^5.3.5",
        "vite": "^2.9.1"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.16.7",
        "@originjs/vite-plugin-commonjs": "^1.0.3",
        "@rollup/plugin-babel": "^5.3.1",
        "cross-env": "^7.0.3",
        "electron": "^18.0.2",
        "electron-builder": "^22.14.13",
        "electron-builder-squirrel-windows": "^23.0.3",
        "electron-webpack": "^2.8.2",
        "fs": "^0.0.1-security",
        "json-server": "^0.17.0",
        "npm-run-all": "^4.1.5",
        "react-error-overlay": "^6.0.10",
        "react-require": "^1.0.1",
        "vite-plugin-electron-renderer": "^0.3.3",
        "vite-plugin-externals": "^0.4.0",
        "vite-plugin-fs": "^1.0.0-beta.3"
    }
}
